[sample ip share]
        comment = Sample IP share
        path = /srv/ip share
        browseable = yes
        read only = no
        guest ok = no
        valid users = @"WORKGROUP+sample group"
        admin users = @"WORKGROUP+sample group"
        public = yes
        inherit owner = yes
        inherit acls = yes
        inherit permissions = yes
        map acl inherit = yes
        locking = no
